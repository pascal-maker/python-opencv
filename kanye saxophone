    {
      "cell_type": "code",
      "source": [
        "import os\n",
        "import cv2\n",
        "import supervision as sv\n",
        "from ultralytics import YOLO\n",
        "\n",
        "# Load the image\n",
        "image = cv2.imread('/content/kanye playing sax.png')\n",
        "\n",
        "# Load the YOLO model\n",
        "model = YOLO('/content/yolov8s.pt')\n",
        "\n",
        "# Run the model on the image\n",
        "results = model(image)[0]\n",
        "\n",
        "# Convert the model's result to the format expected by sv.Detections\n",
        "boxes = results.boxes.xyxy.cpu().numpy()  # Bounding boxes in xyxy format\n",
        "labels = results.boxes.cls.cpu().numpy().astype(int)  # Class labels, converted to integers\n",
        "scores = results.boxes.conf.cpu().numpy()  # Confidence scores\n",
        "\n",
        "# Create the Detections object\n",
        "detections = sv.Detections(xyxy=boxes, class_id=labels, confidence=scores)\n",
        "\n",
        "# Create an output directory\n",
        "output_dir = '/content/supervision/supervision/assets'\n",
        "os.makedirs(output_dir, exist_ok=True)\n",
        "\n",
        "# Annotate the image with bounding boxes\n",
        "bounding_box_annotator = sv.BoundingBoxAnnotator()\n",
        "annotated_frame = bounding_box_annotator.annotate(\n",
        "    scene=image.copy(),\n",
        "    detections=detections\n",
        ")\n",
        "\n",
        "# Save the annotated image\n",
        "output_image_path = os.path.join(output_dir, 'annotated_image.png')\n",
        "cv2.imwrite(output_image_path, annotated_frame)\n",
        "\n",
        "print(f\"Annotated image saved to {output_image_path}\")\n"
      ],
      "metadata": {
